{
  "manifest_version": 2,
  "name": "N2YO Extension",
  "description": "Enlarges elevation, azimuth, local time and UTC time on N2YO tracking pages.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://www.n2yo.com/?s=*","https://www.n2yo.com/?s=*","http://n2yo.com/?s=*","https://n2yo.com/?s=*"],
      "js": ["jquery.min.2.2.4.js","script.js"]
    }
  ]
}
